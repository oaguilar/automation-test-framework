module.exports = {
	  disabled : false,
	
  'Demo test Attensity DiscoveryNow' : function (client) {
    try {
      qcredentials = require('./qcredentials.json');
    } catch (err) {
      console.error('Couldn\'t load the Atttensity DiscoveryNow credentials file. Please ensure that ' +
        'you have the DiscoveryNow credentials.json in the same folder as the test.');
      process.exit();
    }
	
	qconfig = require('./qconfig.json');
		
	var newTopic = qconfig.newTopic;
	var sources = qconfig.sources;
	var allSources = qconfig.allSources;
	var doneButton = qconfig.doneButton;
	var saveTopic = qconfig.saveTopic;
	var home = qconfig.home;
	
	  
	client
	  .click(newTopic)
	  .pause(3000)
	  .setValue('input[type=text]', ['super'])
	  .pause(3000)
	  .keys([client.Keys.SPACE])
	  .pause(2000)
	  .click(sources)
	  .pause(2000)
	  .click(allSources)
	  .pause(2000)
	  .click(doneButton)
	  .pause(10000)
	  .click(saveTopic)
	  .pause(5000)
	  .setValue('input[name=topicName]', ['AAAutoCreatedViaNodeJS'])
	  .pause(3000)
	  .keys([client.Keys.TAB])
	  .keys([client.Keys.TAB])
	  .keys([client.Keys.TAB])
	  .keys([client.Keys.TAB])
	  .keys([client.Keys.ENTER])
	  .pause(1000)
	  .click(home)
	  .pause(30000)
	  .end();
  }
};