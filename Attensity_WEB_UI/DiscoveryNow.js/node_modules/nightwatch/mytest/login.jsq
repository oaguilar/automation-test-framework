module.exports = {
	  disabled : false,
	
  'Demo test Attensity DiscoveryNow' : function (client) {
    try {
      qcredentials = require('./qcredentials.json');
    } catch (err) {
      console.error('Couldn\'t load the Atttensity DiscoveryNow credentials file. Please ensure that ' +
        'you have the DiscoveryNow credentials.json in the same folder as the test.');
      process.exit();
    }
	
	qconfig = require('./qconfig.json');
		
	var stageURL = qconfig.stageURL;
	var loginButton = qconfig.loginButton;
	  
	client
	  .url(stageURL)
	  .pause(5000)
      .waitForElementVisible('body', 9000)
	  .resizeWindow(1680, 1050)
	  .pause(5000)
	  .setValue('input#account', qcredentials.account)
      .setValue('input#login', qcredentials.login)
	  .setValue('input#password', qcredentials.password)
	  .pause(3000)
	  .click(loginButton)
	  .pause(3000)
  }
};